{
  "version": 2,
  "env": {
      "DATABASE_URL": "@amazon_api_postgresql",
      "JWT_SECRET": "@secret_key"
  },
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/main.ts",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "DELETE",
              "PATCH"
          ]
      }
  ]
}
{

  "version": 2,

  "builds": [

      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [

      {
          "src": "/api/(.*)",
          "dest": "src/main.ts",
          "methods": [
              "OPTIONS",
              "GET",
              "POST",
              "PUT",
              "DELETE",
              "PATCH"
          ]
      }
      

  ]

}